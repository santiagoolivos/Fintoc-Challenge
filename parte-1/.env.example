# Fintoc API Configuration
FINTOC_API_KEY=sk_test_your_api_key_here
FINTOC_BASE_URL=https://api.fintoc.com/v2

# JWS Private Key (required for transfers)
# Generate with: openssl genrsa -out private_key.pem 2048
# Then upload public key to Fintoc dashboard
FINTOC_JWS_PRIVATE_KEY_PATH=./private_key.pem

# Your Fintoc Account ID (source account for transfers)
FINTOC_ACCOUNT_ID=acc_your_account_id_here
